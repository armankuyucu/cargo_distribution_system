<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.yazlab1_proje1.SignUpController">
   <children>
      <TextField fx:id="userNameTextField" layoutX="226.0" layoutY="141.0" prefHeight="20.0" prefWidth="194.0" />
      <Label fx:id="statusLabel" alignment="CENTER" layoutY="51.0" prefHeight="53.0" prefWidth="601.0" text="Sign Up">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="153.0" layoutY="144.0" text="Username">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="153.0" layoutY="206.0" text="Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="passwordTextField" layoutX="226.0" layoutY="203.0" onAction="#submit" prefHeight="25.0" prefWidth="194.0" />
      <Button layoutX="270.0" layoutY="253.0" mnemonicParsing="false" onAction="#submit" text="Submit">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchBack" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
